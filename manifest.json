{
  "name": "VS Code Triage",
  "version": "0.2.1",
  "manifest_version": 2,
  "description": "A chromium extension to help with triaging VS Code GitHub issues.",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_popup": "src/popup/popup.html"
  },
  "options_page": "src/options/options.html",
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "js": [
        "out/content.js"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L"
      }
    }
  }
}
